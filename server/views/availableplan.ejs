<body class="center-menu">

    <% include ./partials/navbar %>


    <!-- ============================================================== -->
    <!-- Start Page Content here -->
    <!-- ============================================================== -->
    <div class="wrapper">

        <div class="container-fluid">
            <div class="container-fluid">
                <div class="mt-3">
                    <% include ./partials/messages %>
                </div>

            <!-- start page title -->
            <div class="row">
                <div class="col-12">
                    <div class="page-title-box">
                        <h4 class="page-title">All Submissions</h4>
                    </div>
                </div>
            </div>
            <!-- end page title -->



            <div class="col-xl-12">
                <div class="card mb-4">
                    <div class="card-body">
                        <table class="table table-stripped">
                            <thead>
                                <th>No.</th>
                                <th>Title</th>
                                <th>Due Date</th>
                                <th>Actions</th>
                            </thead>
                            <tbody>
                                <% for(let i = 0; i<plans.length; i++) { %>
                                <tr>
                                    <td><%= i + 1 %></td>
                                    <td><%= plans[i].title %></td>
                                    <td><%= plans[i].dueDate %></td>
                                    <td><a href="take/survey/<%= plans[i]._id %>" >View >></a></td>
                                </tr>
                                
                            </tbody>
                        </table>                        
                    </div> <!-- end card-body -->
                </div> <!-- end card-->
            </div> 
            <!-- end col -->
        </div> <!-- end card-->
    </div> <!-- end col -->

    </div> <!-- end container fluid-->
    </div><!-- end wrapper-->



    <!-- ============================================================== -->
    <!-- End Page content -->
    <!-- ============================================================== -->
</body>

</html>