<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8" />
  <title>Adminto - Responsive Admin Dashboard Template</title>
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta content="A fully featured admin theme which can be used to build CRM, CMS, etc." name="description" />
  <meta content="Coderthemes" name="author" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
  <!-- App favicon -->
  <link rel="shortcut icon" href="assets/images/favicon.ico">

  <!-- App css -->
  <link href="/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
  <link href="/css/icons.min.css" rel="stylesheet" type="text/css" />
  <link href="/css/app.css" rel="stylesheet" type="text/css" />

</head>

<body class="authentication-bg">


  <div class="account-pages mt-5 mb-5">
    <div class="container">
      <div class="row justify-content-center">
        <div class="col-md-8 col-lg-6 col-xl-5">
          <!-- <div class="text-center">
            <span><img src= "/assets/img/CAN_logo_web.png" alt="" height=100px width="auto"></span>
        </div> -->
          <div class="card card-login">

            <div class="card-body p-4">

              <div class="text-center mb-4">
                <h4 class="text-uppercase mt-0">CREATE ACCOUNT</h4>
              </div>
              <% include ../partials/messages %>
              <form action="/users/register" method="POST">

                <div class="form-group">
                  <label for="code">Organization ID</label>
                  <input class="form-control" type="code" id="code" name="code" placeholder="Enter Your Organization ID" value="<%= typeof code != 'undefined' ? code : '' %>">
                </div>

                <div class="form-group">
                  <label for="name">Organization Name</label>
                  <input class="form-control" type="text" id="name" name="name" placeholder="Enter Your Organization Name" value="<%= typeof name != 'undefined' ? name : '' %>">
                </div>

                <div class="form-group">
                  <label for="name">Primary Contact Name</label>
                  <input class="form-control" type="text" id="contact" name="contactName" placeholder="Enter Your Primary Contact Name" value="<%= typeof contactName != 'undefined' ? contactName : '' %>">
                </div>

                <div class="form-group">
                  <label for="email">Primary Contact Email</label>
                  <input class="form-control" type="email" id="email" name="email" required placeholder="Enter Your Primary Contact Email" value="<%= typeof email != 'undefined' ? email : '' %>">
                </div>

                <div class="form-group">
                  <label for="password">Password</label>
                  <p>Minimum 6 characters, and contain at least 1 uppercase and 1 special character. </p>
                  <input class="form-control" type="password" required id="password" name="password" placeholder="Enter Your Password" value="<%= typeof password != 'undefined' ? password : '' %>">
                </div>

                <div class="form-group">
                  <label for="password2">Confirm Password</label>
                  <input class="form-control" type="password" required id="password2" name="password2" placeholder="Confirm Your Password" value="<%= typeof password2 != 'undefined' ? password2 : '' %>">
                </div>


                <div class="form-group mb-0 text-center">
                  <button class="btn btn-custom-color btn-block" type="submit"> Sign Up </button>
                </div>

              </form>

            </div> <!-- end card-body -->
          </div>
          <!-- end card -->

          <div class="row mt-3">
            <div class="col-12 text-center">
              <p class="text-dark">Already have an account? <a href="/" class="text-dark ml-1"><b>Sign
                    In</b></a></p>
            </div> <!-- end col -->
          </div>
          <!-- end row -->

        </div> <!-- end col -->
      </div>
      <!-- end row -->
    </div>
    <!-- end container -->
  </div>
  <!-- end page -->

  <!-- Vendor js -->
  <script src="/js/vendor.min.js"></script>

  <!-- App js-->
  <script src="/js/app.min.js"></script>




</body>

</html>